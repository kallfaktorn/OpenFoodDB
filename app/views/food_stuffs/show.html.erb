<%= link_to image_tag('Thumb up!', :border => 0), vote_up_food_stuff_path(@food_stuff), :remote => true, :id => "thumbs_up" %>
<label id="votes_id"><%= @food_stuff.votes_for %></label>

<%= render :partial => 'comment', :collection => @food_stuff.comments %>

<%= form_for([@food_stuff, @food_stuff.comments.build]) do |f| %>
  <%= f.hidden_field :food_stuff_id %>

  <div class="field">
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </div>

  <div class="actions">
    <%= f.submit "Post Comment" %>
  </div>
<% end %>


<%= link_to 'Edit', edit_food_stuff_path(@food_stuff) %> |
<%= link_to 'Back', home_index_path %>


<script type="text/javascript">
  $('#thumbs_up').bind('ajax:success', function(evt, data) {
  	$('#votes_id').text(data);
  });
</script>